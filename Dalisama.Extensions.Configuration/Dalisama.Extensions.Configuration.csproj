<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <TargetFramework>netstandard2.0</TargetFramework>
    <id>Dalisama.Extensions.Configuration</id>
    <version>1.0.0</version>
    <title>Dalisama.Extensions.Configuration</title>
    <authors>Mohamed Ali Kachbouri (dalisama)</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license>MIT</license>
    <projectUrl>https://github.com/dalisama/Dalisama.Extensions.Configuration</projectUrl>
    <icon>https://dalisama.me/wp-content/uploads/2018/09/cropped-eyeglasses1-32x32.png</icon>
