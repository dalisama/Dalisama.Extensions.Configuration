    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/dalisama/Dalisama.Extensions.Configuration</PackageProjectUrl>
    <PackageIcon>cropped-eyeglasses1-192x192.png</PackageIcon>
    <RepositoryUrl>https://github.com/dalisama/Dalisama.Extensions.Configuration</RepositoryUrl>
    <PackageTags>dotnetcore; custom configuration provider</PackageTags>
